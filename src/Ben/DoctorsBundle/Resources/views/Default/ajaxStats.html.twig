{% if stats.cnss is defined %}
	<div class="col-md-6">  
	    <div class="box">
		    <header>
		        <h4><span class="glyphicon glyphicon-file"></span> Medical insurance</h4>
		    </header>
		    <table class="table table-striped table-bordered">
		    <thead>
			    <tr>
			    	<th>Type of insurance</th>
			    	<th>Number</th>
			    </tr>
		    </thead>
		        {% for item in stats.cnss %}
		        <tr>
		            <td>{{ item.label }}</td>
		            <td>{{ item.data }}</td>
		        </tr>
		        {% endfor %}
		    </table>
	    </div>
	</div>
{% endif %}  
